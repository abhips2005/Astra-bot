{
  "name": "asthra-bot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:secure": "concurrently \"vite --host\" \"wait-on tcp:5173 && node scripts/secure-tunnel.js\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-cert": "node scripts/generate-cert.js",
    "setup-https": "node scripts/setup-https.js",
    "dev:tunnel": "concurrently \"vite --host\" \"cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node scripts/lt-tunnel.js\"",
    "proxy": "node scripts/proxy-server.js",
    "tunnel": "node scripts/proxy-tunnel.js",
    "dev:proxy": "concurrently \"npm run dev\" \"npm run proxy\" \"npm run tunnel\"",
    "setup:https": "node scripts/generate-cert.js",
    "dev:https": "npm run setup:https && concurrently \"npm run dev\" \"npm run proxy\" \"npm run tunnel\"",
    "dev:cf": "concurrently --kill-others \"vite --host\" \"wait-on tcp:5173 && node scripts/cloudflare-tunnel.js\"",
    "tunnel:server": "node scripts/tunnel-server.js",
    "tunnel:client": "node scripts/custom-tunnel.js",
    "dev:custom": "concurrently \"npm run tunnel:server\" \"wait-on tcp:3000 && npm run tunnel:client\" \"vite --host\"",
    "dev:ngrok": "concurrently \"vite --host\" \"wait-on tcp:5173 && node scripts/ngrok-tunnel.js\"",
    "standalone:tunnel": "concurrently \"vite --host\" \"wait-on tcp:5173 && node scripts/standalone-tunnel.js\""
  },
  "dependencies": {
    "date-fns": "^3.3.1",
    "framer-motion": "^11.0.8",
    "lucide-react": "^0.344.0",
    "node-forge": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-webcam": "^7.2.0",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^4.21.2",
    "http-proxy-middleware": "^2.0.7",
    "https": "^1.0.0",
    "localtunnel": "^2.0.2",
    "ngrok": "^4.3.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-plugin-pwa": "^0.21.1",
    "wait-on": "^7.2.0"
  }
}
